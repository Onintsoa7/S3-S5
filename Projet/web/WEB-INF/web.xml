        <servlet-name>ServletSupprimeStyle</servlet-name>
        <url-pattern>/ServletSupprimeStyle</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletTaille</servlet-name>
        <url-pattern>/ServletTaille</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servletformule</servlet-name>
        <url-pattern>/Servletformule</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletAfficheFinal</servlet-name>
        <url-pattern>/ServletAfficheFinal</url-pattern>
    </servlet-mapping>
