    </servlet>
    <servlet>
        <servlet-name>ServletTaille</servlet-name>
        <servlet-class>Controller.ServletTaille</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servletformule</servlet-name>
        <servlet-class>Controller.Servletformule</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletAfficheFinal</servlet-name>
        <servlet-class>Controller.ServletAfficheFinal</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletLogin</servlet-name>
        <url-pattern>/ServletLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletCategorie</servlet-name>
        <url-pattern>/ServletCategorie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletStyle</servlet-name>
        <url-pattern>/ServletStyle</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletMateriel</servlet-name>
        <url-pattern>/ServletMateriel</url-pattern>
